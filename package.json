{
  "name": "arrpc",
  "version": "3.5.0",
  "description": "Open Discord RPC server for atypical setups",
  "main": "src/server.js",
  "scripts": {
    "start": "bun ./updateWorkerString.js && node ./src/index.js",
    "bundleRun": "bun ./updateWorkerString.js && bun build ./src/index.js --target node --format esm --packages bundle --minify --outdir build && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAsar/arrpc.git"
  },
  "author": "OpenAsar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenAsar/arrpc/issues"
  },
  "homepage": "https://github.com/OpenAsar/arrpc#readme",
  "dependencies": {
    "picocolors": "^1.1.1",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "type": "module",
  "bin": {
    "arrpc": "src/index.js"
  }
}